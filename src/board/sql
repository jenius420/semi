CREATE OR REPLACE VIEW bList
AS
(SELECT ROWNUM RNUM, A.* FROM (SELECT TNUM, TITLE, ENUM, ENAME, UPDATEDATE, BBODY, INVALIDPOST, ISNOTICE, BOARDCOUNT FROM BOARD B JOIN EMPLOYEE USING(ENUM) WHERE INVALIDPOST='N' ORDER BY TNUM DESC) A);


